<launch>
	<node name="eross_control_node" pkg="cepheus_robot" type="eross_control_node" output="screen" required="true">
		<param name="loop_rate" value="200.0"/>
	</node>
    <node name="record_topics_for_verification" pkg="rosbag" type="record" args="-o $(find cepheus_robot)/bags/cepheus  /secs 
																														/set_left_shoulder_effort 
																														/set_left_elbow_effort 
																														/set_right_elbow_effort 
																														/set_left_shoulder_qd 
																														/set_left_elbow_qd 
																														/set_right_elbow_qd 
																														/read_left_shoulder_position 
																														/read_left_elbow_position 
																														/read_right_elbow_position 
																														/read_left_shoulder_velocity 
																														/read_left_elbow_velocity 
																														/read_right_elbow_velocity
																														/left_shoulder_pos
																														/left_elbow_pos
																														/right_elbow_pos
																														/left_shoulder_vel
																														/left_elbow_vel
																														/right_elbow_vel
																														/FSR_soft
																														/FSR_hard
																														/ls_error
																														/le_error
																														/re_error
																														/g_x
																														/g_y
																														/g_th
																														/g_x_dot
																														/g_y_dot
																														/g_th_dot
																														/g_x_desdot
																														/c_x
																														/c_y
																														/c_th
																														/g_error_x
																														/g_error_y
																														/g_error_th
																														/det
																														/map_to_alxgripper" respawn="true" output="screen"/>

</launch>

