<launch>
	<node name="obsidian_bp2_node" pkg="cepheus_robot" type="obsidian_bp2_node" output="screen" required="true">
		<param name="loop_rate" value="200.0"/>
	</node>
	<node name="record_topics_for_verification" pkg="rosbag" type="record" args="-o $(find cepheus_robot)/bags/cepheus  /secs
                                                                                                                        /ls_torque 
																														/le_torque 
																														/rw_torque
																														/left_shoulder_pos
																														/left_elbow_pos
																														/left_shoulder_vel
																														/left_elbow_vel
																														/reaction_wheel_pos
																														/reaction_wheel_vel
																														/reaction_wheel_vell
                                                                                                                        /qe_des_x
                                                                                                                        /qe_des_y
                                                                                                                        /qe_dot_des_x
                                                                                                                        /qe_dot_des_y
                                                                                                                        /qe_dotdot_des_x
                                                                                                                        /qe_dotdot_des_y
																														/ps_theta
																														/ps_theta_dot
																														/imu_angular_vel
																														/imu_orient
																														/imu_vel_x
																														/imu_vel_y
																														/imu_acc_x
																														/imu_acc_y
																														/c_x
																														/c_y" respawn="true" output="screen"/>
</launch>
